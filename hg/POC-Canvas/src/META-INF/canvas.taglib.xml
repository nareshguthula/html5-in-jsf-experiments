<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib id="o"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
	version="2.0"
>
	<namespace>http://jdevelopment.nl</namespace>
	
	<tag>
		<tag-name>canvasgraph</tag-name>
		<component>
			<component-type>component.CanvasGraph</component-type>
		</component>
		
		<attribute>
			<name>width</name>
			<required>false</required>
			<type>java.lang.Integer</type>
		</attribute>
		
		<attribute>
			<name>height</name>
			<required>false</required>
			<type>java.lang.Integer</type>
		</attribute>
		
		<attribute>
			<name>style</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		
		<attribute>
			<name>lineColor</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		
		<attribute>
			<name>backgroundColor</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		
		<attribute>
			<name>axesColor</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		
		
		<attribute>
			<name>pointColor</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		
		<attribute>
			<name>table</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		
		<attribute>
			<name>idColumn</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		
		<attribute>
			<name>valueColumn</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		
		<attribute>
			<name>visibleCheckbox</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		
		<attribute>
			<name>binding</name>
			<required>false</required>
			<type>javax.faces.component.UIComponent</type>
		</attribute>
		
		<attribute>
			<name>onclick</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		
		<attribute>
			<name>value</name>
			<required>false</required>
			<type>java.lang.Object</type>
		</attribute>
	</tag>
	
	<tag>
		<tag-name>highlightRow</tag-name>
		
		<behavior>
			<behavior-id>component.HighlightRow</behavior-id>
		</behavior>
		
		<attribute>
			<name>event</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		
		<attribute>
			<name>cssClass</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
	</tag>
</facelet-taglib>